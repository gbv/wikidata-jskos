{
  "name": "wikidata-jskos",
  "version": "0.3.2",
  "jskos-api": "1.0",
  "description": "Access Wikidata in JSKOS format",
  "main": "server.js",
  "bin": {
    "wdjskos": "./bin/wdjskos"
  },
  "scripts": {
    "start": "nodemon server.js",
    "server": "node server.js",
    "update": "./bin/wdjskos update && cp cache/mapping-schemes.ndjson assets",
    "test": "mocha --timeout 30000",
    "test-watch": "mocha --watch",
    "lint": "eslint '*.js' '**/*.js'",
    "fix": "eslint --fix '*.js' '**/*.js'",
    "lint-staged": "lint-staged",
    "home": "./scripts/build_home"
  },
  "files": [
    "lib/*",
    "assets/*",
    "test/*",
    "scripts/*",
    "bin/*",
    "*.md",
    "*.js",
    "*.json"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix"
    ],
    "src/*.js": [
      "eslint --fix"
    ]
  },
  "pre-commit": [
    "home",
    "lint-staged"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gbv/wikidata-jskos"
  },
  "bugs": {
    "url": "https://github.com/gbv/wikidata-jskos/issues"
  },
  "homepage": "https://github.com/gbv/wikidata-jskos",
  "keywords": [
    "wikidata",
    "jskos",
    "skos"
  ],
  "author": "Jakob Vo√ü",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "lint-staged": "^10.2.11",
    "marked": "^1.1.1",
    "mocha": "^7.2.0",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jskos-tools": "^1.0.9",
    "lodash": "^4.17.19",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "simple-sha1": "^3.0.1",
    "wikibase-edit": "^4.6.1",
    "wikidata-sdk": "^7.7.0"
  }
}
