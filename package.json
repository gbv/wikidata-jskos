{
  "name": "wikidata-jskos",
  "version": "0.1.2",
  "description": "Access Wikidata in JSKOS format",
  "main": "server.js",
  "bin": {
    "wdjskos": "./bin/wdjskos"
  },
  "scripts": {
    "start": "forever --minUptime 2000 --spinSleepTime 2000 -c 'nodemon --exitcrash' server.js",
    "server": "node server.js",
    "test": "mocha --timeout 10000",
    "test-watch": "mocha --watch",
    "lint": "eslint '*.js' '**/*.js'",
    "fix": "eslint --fix '*.js' '**/*.js'",
    "lint-staged": "lint-staged",
    "home": "./scripts/build_home"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "src/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "home",
    "lint-staged"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gbv/wikidata-jskos"
  },
  "bugs": {
    "url": "https://github.com/gbv/wikidata-jskos/issues"
  },
  "homepage": "https://github.com/gbv/wikidata-jskos",
  "keywords": [
    "wikidata",
    "jskos",
    "skos"
  ],
  "author": "Jakob Vo√ü",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.16.0",
    "lint-staged": "^7.1.2",
    "marked": "^0.6.2",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2",
    "should": "^13.2.1"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "debug": "^4.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "forever": "^1.0.0",
    "jskos-tools": "^0.2.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.17.5",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "simple-sha1": "^2.1.1",
    "wikidata-sdk": "^7.0.0"
  }
}
