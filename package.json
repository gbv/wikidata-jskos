{
  "name": "wikidata-jskos",
  "version": "0.0.1",
  "description": "Access Wikidata in JSKOS format",
  "main": "server.js",
  "bin": {
    "wdjskos": "./bin/wdjskos"
  },
  "scripts": {
    "start": "forever --minUptime 2000 --spinSleepTime 2000 -c 'nodemon --exitcrash' server.js",
    "server": "node server.js",
    "test": "mocha --timeout 5000",
    "test-watch": "mocha --watch",
    "lint": "./node_modules/.bin/eslint ./*.js ./**/*.js",
    "fix": "./node_modules/.bin/eslint --fix ./*.js ./**/*.js",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "pre-commit": "lint-staged",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbv/wikidata-jskos"
  },
  "bugs": {
    "url": "https://github.com/gbv/wikidata-jskos/issues"
  },
  "homepage": "https://github.com/gbv/wikidata-jskos",
  "keywords": [
    "wikidata",
    "jskos",
    "skos"
  ],
  "author": "Jakob Vo√ü",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.19.1",
    "lint-staged": "^7.1.2",
    "pre-commit": "^1.2.2",
    "should": "^13.2.1",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "bluereq": "^2.1.7",
    "chalk": "^1.1.3",
    "config": "^1.30.0",
    "debug": "^4.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "forever": "^0.15.3",
    "jskos-tools": "^0.1.6",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.17.5",
    "simple-sha1": "^2.1.1",
    "wikidata-sdk": "^5.12.0"
  }
}
